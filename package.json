    "@types/puppeteer": "^1.11.1",
    "@types/webpack": "^4.4.22",
    "@types/webpack-merge": "^4.1.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-template-html-minifier": "^2.1.3",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
