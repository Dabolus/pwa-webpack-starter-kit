{
  "name": "pwa-webpack-starter-kit",
  "version": "1.0.0",
  "description": "PWA Starter Kit with Webpack",
  "author": "Giorgio Garasto <giorgio@garasto.it>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "gulp": "gulp -f gulp/config.ts --cwd .",
    "start": "cross-env BROWSERSLIST=\"edge > 12\" webpack-dev-server --config webpack/dev.config.ts --hot --color --compress --overlay --history-api-fallback --open",
    "build:static": "webpack --config webpack/prod.config.ts",
    "build": "npm run gulp build",
    "serve": "prpl-server --root build/ --host 0.0.0.0 --https-redirect --bot-proxy https://render-tron.appspot.com/render",
    "lint": "npm run gulp lint",
    "test": "cross-env NODE_ENV=test JEST_PUPPETEER_CONFIG=jest/puppeteer.config.js jest -c jest/base.config.js",
    "test:integration": "cross-env NODE_ENV=test JEST_PUPPETEER_CONFIG=jest/puppeteer.config.js jest -c jest/integration.config.js",
    "test:unit": "cross-env NODE_ENV=test jest -c jest/unit.config.js"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.2.0",
    "@polymer/app-layout": "^3.0.2",
    "@polymer/lit-element": "^0.6.5",
    "@polymer/polymer": "^3.1.0",
    "@webcomponents/webcomponentsjs": "^2.2.2",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@piuccio/ejs-compiled-loader": "^3.0.0",
    "@skatejs/ssr": "^0.19.11",
    "@types/clean-webpack-plugin": "^0.1.2",
    "@types/copy-webpack-plugin": "^4.4.2",
    "@types/expect-puppeteer": "^3.3.0",
    "@types/gulp": "^4.0.5",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^23.3.12",
    "@types/jest-axe": "^2.2.2",
    "@types/jest-environment-puppeteer": "^2.2.1",
